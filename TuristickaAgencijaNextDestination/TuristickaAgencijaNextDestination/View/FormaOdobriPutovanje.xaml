<Window x:Class="TuristickaAgencijaNextDestination.View.FormaOdobriPutovanje"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="FormaOdobriPutovanje" Height="342" Width="789">
    <Grid Margin="0,0,0,0">
        <DataGrid x:Name="gridOdobriPutovanja" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" Height="190" Width="761"/>
        <Button x:Name="btnPrikazi" Content="Prikazi" HorizontalAlignment="Left" Margin="180,252,0,0" VerticalAlignment="Top" Width="130" RenderTransformOrigin="-0.2,-0.25" Click="Button_Click_1"/>
        <Button x:Name="btnOdobri" Content="Odobri" HorizontalAlignment="Left" Margin="665,275,0,0" VerticalAlignment="Top" Width="90" Click="btnOdobri_Click"/>
        <RadioButton x:Name="rbPutovanjeSaizletom" Content="Putovanja sa izletom" HorizontalAlignment="Left" Margin="10,230,0,0" VerticalAlignment="Top"/>
        <RadioButton x:Name="rbPutovanjeBezIzleta" Content="Putovanja bez izleta" HorizontalAlignment="Left" Margin="10,255,0,0" VerticalAlignment="Top"/>
        <Button Content="Nazad" HorizontalAlignment="Left" Margin="570,275,0,0" VerticalAlignment="Top" Width="90" Click="Button_Click_3"/>

    </Grid>
</Window>
